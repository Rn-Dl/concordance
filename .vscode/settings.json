    "python.linting.pylintEnabled": false,
    "python.linting.pep8Enabled": true,
    "python.formatting.provider": "autopep8",
    "C_Cpp.clang_format_formatOnSave": true,
    "C_Cpp.clang_format_sortIncludes": null,
    "editor.formatOnType": true
}